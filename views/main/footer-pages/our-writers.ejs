    var scriptsArray = ["\/js\/cookie-dialog.bcf194b3a41f3dcdf706.js", "#\/js\/our-writers-9a8b795a.d74d8ad598cef0f63416.js", "#\/js\/our-writers-c01911d2.29ddc905a965fb3c05cf.js", "#\/js\/our-writers-ff7f2a21.8faa708a0bf3730ba8b3.js", "#\/js\/our-writers-9c97f886.9b905741d68d04f44d91.js", "\/js\/our-writers-4138be91.61e8fb9d6988a0394ddb.js"];

    var body = document.getElementsByTagName('body')[0];
    window.addEventListener('load', function () {
      for (var scriptLink in scriptsArray) {
        var scriptEl = document.createElement('script');
        scriptEl.async = true;
        scriptEl.src = scriptsArray[scriptLink];
        body.appendChild(scriptEl);
      }
    });

    document.addEventListener('DOMContentLoaded', function () {
      // Get all the slide buttons
      const slideButtons = document.querySelectorAll('.splide__pagination__page');

      // Get the list of slides
      const slideList = document.querySelector('.splide__list');

      // Get all the slides
      const slides = document.querySelectorAll('.splide__slide');

      // Function to handle slide transition
      function goToSlide(index) {
        // Calculate the offset to translate the slide container
        const slideWidth = slides[0].clientWidth;
        const slideMargin = parseInt(window.getComputedStyle(slides[0]).marginRight);
        const translateX = -(slideWidth + slideMargin) * index;

        // Apply the transformation
        slideList.style.transform = `translateX(${translateX}px)`;

        // Update the active button
        slideButtons.forEach(button => button.classList.remove('is-active'));
        slideButtons[index].classList.add('is-active');

        // Update the aria-selected attributes for accessibility
        slideButtons.forEach(button => button.setAttribute('aria-selected', 'false'));
        slideButtons[index].setAttribute('aria-selected', 'true');
      }

      // Add event listeners to all the pagination buttons
      slideButtons.forEach((button, index) => {
        button.addEventListener('click', () => {
          goToSlide(index);
        });
      });
    });

  </script>




